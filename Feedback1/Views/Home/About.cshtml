@{
  ViewData["Title"] = "About";
}




<script type="text/javascript">

  $(function () {

    var customers = [
      {
        "id": "1",
        "value": "Urban Development",
        "label": "Urban Development"
      },
      {
        "id": "17",
        "value": "Ball Corporation",
        "label": "Ball Corporation"
      },
      {
        "id": "18",
        "value": "The Coca-Cola Company",
        "label": "The Coca-Cola Company"
      },
      {
        "id": "20",
        "value": "Discovery Communications, Inc.",
        "label": "Discovery Communications, Inc."
      },
      {
        "id": "22",
        "value": "Electronic Data Systems",
        "label": "Electronic Data Systems"
      },
      {
        "id": "10",
        "value": "FreeWave Technologies, Inc.",
        "label": "FreeWave Technologies, Inc."
      }];

    $("#field").autocomplete({
      source: customers,
      minLength: 1,
      select: function (event, ui) {
        $("#field_id").val(ui.item.id);
      }
    });
  });

</script>
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<p>Use this area to provide additional information.</p>


<h3>Please enter you postcode</h3>
<form id="simdsearch">

  <label for="field">Your postcode</label>
  <input id="field" type="text" style="width: 200px;">

  <label for="field_id">ID</label>
  <input id="field_id" type="text" style="width: 50px;">

</form>


